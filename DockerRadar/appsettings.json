{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UpdateCheck": {
    "IntervalMinutes": 5,
    "UpdateRangeFrom": 45,
    "UpdateRangeTo": 90
  },
  "Provider": {
    "ghcr.io": {
      "Enabled": true,
      "Username": "<username>",
      "Token": "<something>"
    },
    "docker.io": {
      "Enabled": true
    },
    "mcr.microsoft.com": {
      "Enabled": true
    },
    "lscr.io": {
      "Enabled": true
    }
  }
}
